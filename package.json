    "@testing-library/react": "^14.0.0",
    "@types/chrome": "^0.0.216",
    "@types/cross-spawn": "^6.0.2",
    "@types/figlet": "^1.5.5",
    "@types/jest": "^29.4.0",
    "@types/js-beautify": "^1.13.3",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
