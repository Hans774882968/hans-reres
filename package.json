    "@uiw/codemirror-theme-gruvbox-dark": "^4.19.10",
    "@uiw/react-codemirror": "^4.19.10",
    "ahooks": "^3.7.5",
    "antd": "^5.2.1",
    "i18next": "^22.4.10",
    "i18next-browser-languagedetector": "^7.0.1",
    "js-beautify": "^1.14.7",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0"
  },
  "devDependencies": {
